# Токен вашего Telegram бота (получите у @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# ID чата, в который будет отправляться сообщение (можно оставить пустым и использовать /setchat)
CHAT_ID=

# Дата начала службы в формате ГГГГ-ММ-ДД
ARMY_START_DATE=2023-06-16

# Длительность службы в днях
ARMY_SERVICE_LENGTH_DAYS=365

# Порт для веб-сервера (Render автоматически устанавливает этот порт)
PORT=3000 